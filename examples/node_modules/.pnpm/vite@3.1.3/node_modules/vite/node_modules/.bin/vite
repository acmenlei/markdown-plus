#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Library/coderlei-pro/coderlei-pro-example/plugins/markdown-parse-gen-menu/examples/node_modules/.pnpm/vite@3.1.3/node_modules/vite/bin/node_modules:/Library/coderlei-pro/coderlei-pro-example/plugins/markdown-parse-gen-menu/examples/node_modules/.pnpm/vite@3.1.3/node_modules/vite/node_modules:/Library/coderlei-pro/coderlei-pro-example/plugins/markdown-parse-gen-menu/examples/node_modules/.pnpm/vite@3.1.3/node_modules:/Library/coderlei-pro/coderlei-pro-example/plugins/markdown-parse-gen-menu/examples/node_modules/.pnpm/node_modules:/Library/coderlei-pro/coderlei-pro-example/plugins/markdown-parse-gen-menu/examples/node_modules:/Library/coderlei-pro/coderlei-pro-example/plugins/markdown-parse-gen-menu/node_modules:/Library/coderlei-pro/coderlei-pro-example/plugins/node_modules:/Library/coderlei-pro/coderlei-pro-example/node_modules:/Library/coderlei-pro/node_modules:/Library/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Library/coderlei-pro/coderlei-pro-example/plugins/markdown-parse-gen-menu/examples/node_modules/.pnpm/vite@3.1.3/node_modules/vite/bin/node_modules:/Library/coderlei-pro/coderlei-pro-example/plugins/markdown-parse-gen-menu/examples/node_modules/.pnpm/vite@3.1.3/node_modules/vite/node_modules:/Library/coderlei-pro/coderlei-pro-example/plugins/markdown-parse-gen-menu/examples/node_modules/.pnpm/vite@3.1.3/node_modules:/Library/coderlei-pro/coderlei-pro-example/plugins/markdown-parse-gen-menu/examples/node_modules/.pnpm/node_modules:/Library/coderlei-pro/coderlei-pro-example/plugins/markdown-parse-gen-menu/examples/node_modules:/Library/coderlei-pro/coderlei-pro-example/plugins/markdown-parse-gen-menu/node_modules:/Library/coderlei-pro/coderlei-pro-example/plugins/node_modules:/Library/coderlei-pro/coderlei-pro-example/node_modules:/Library/coderlei-pro/node_modules:/Library/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
