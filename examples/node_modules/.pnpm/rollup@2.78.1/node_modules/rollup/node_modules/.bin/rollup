#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Library/coderlei-pro/coderlei-pro-example/plugins/markdown-parse-gen-menu/examples/node_modules/.pnpm/rollup@2.78.1/node_modules/rollup/dist/bin/node_modules:/Library/coderlei-pro/coderlei-pro-example/plugins/markdown-parse-gen-menu/examples/node_modules/.pnpm/rollup@2.78.1/node_modules/rollup/dist/node_modules:/Library/coderlei-pro/coderlei-pro-example/plugins/markdown-parse-gen-menu/examples/node_modules/.pnpm/rollup@2.78.1/node_modules/rollup/node_modules:/Library/coderlei-pro/coderlei-pro-example/plugins/markdown-parse-gen-menu/examples/node_modules/.pnpm/rollup@2.78.1/node_modules:/Library/coderlei-pro/coderlei-pro-example/plugins/markdown-parse-gen-menu/examples/node_modules/.pnpm/node_modules:/Library/coderlei-pro/coderlei-pro-example/plugins/markdown-parse-gen-menu/examples/node_modules:/Library/coderlei-pro/coderlei-pro-example/plugins/markdown-parse-gen-menu/node_modules:/Library/coderlei-pro/coderlei-pro-example/plugins/node_modules:/Library/coderlei-pro/coderlei-pro-example/node_modules:/Library/coderlei-pro/node_modules:/Library/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Library/coderlei-pro/coderlei-pro-example/plugins/markdown-parse-gen-menu/examples/node_modules/.pnpm/rollup@2.78.1/node_modules/rollup/dist/bin/node_modules:/Library/coderlei-pro/coderlei-pro-example/plugins/markdown-parse-gen-menu/examples/node_modules/.pnpm/rollup@2.78.1/node_modules/rollup/dist/node_modules:/Library/coderlei-pro/coderlei-pro-example/plugins/markdown-parse-gen-menu/examples/node_modules/.pnpm/rollup@2.78.1/node_modules/rollup/node_modules:/Library/coderlei-pro/coderlei-pro-example/plugins/markdown-parse-gen-menu/examples/node_modules/.pnpm/rollup@2.78.1/node_modules:/Library/coderlei-pro/coderlei-pro-example/plugins/markdown-parse-gen-menu/examples/node_modules/.pnpm/node_modules:/Library/coderlei-pro/coderlei-pro-example/plugins/markdown-parse-gen-menu/examples/node_modules:/Library/coderlei-pro/coderlei-pro-example/plugins/markdown-parse-gen-menu/node_modules:/Library/coderlei-pro/coderlei-pro-example/plugins/node_modules:/Library/coderlei-pro/coderlei-pro-example/node_modules:/Library/coderlei-pro/node_modules:/Library/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin/rollup" "$@"
else
  exec node  "$basedir/../../dist/bin/rollup" "$@"
fi
